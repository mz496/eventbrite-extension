{
  "manifest_version": 2,
  "name": "Eventbrite Chrome Extension",
  "description": "Chrome Extension",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "public/index.html"
  },
  "permissions": [
    "geolocation",
    "https://maps.googleapis.com/",
    "https://www.eventbriteapi.com/"
  ],
  "content_security_policy": "script-src 'self' https://maps.googleapis.com https://www.eventbriteapi.com; object-src 'self'"
}